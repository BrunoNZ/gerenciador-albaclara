<div class="col-lg-6">
<div class="well bs-component live-less-editor-hovercontainer" >
<%= simple_form_for(@supervisor) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, :label => "Nome" %>
    <%= f.input :email, :label => "Email" %>
    <%= f.input :phone, :label => "Telefone" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Salvar", class: "btn btn-primary" %>
    <%= link_to "Voltar", :back,  class: "btn btn-warning" %>
  </div>
<% end %>
</div>
</div>
