<p id="notice"><%= notice %></p>

<h1>Listing Report Schedules</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @report_schedules.each do |report_schedule| %>
      <tr>
        <td><%= link_to 'Show', report_schedule %></td>
        <td><%= link_to 'Edit', edit_report_schedule_path(report_schedule) %></td>
        <td><%= link_to 'Destroy', report_schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_report_schedule_path %>
